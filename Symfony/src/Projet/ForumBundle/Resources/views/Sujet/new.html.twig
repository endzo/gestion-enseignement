{% extends "ProjetForumBundle::layout.html.twig" %}

{% block title %}
	Nouveau - {{ parent() }}
{% endblock %}

{% block Forum_body %}

<h1>Sujet creation</h1>

<form action="{{ path('enseignement_createSujet', { 'id': id }) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-success">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('sujet') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}