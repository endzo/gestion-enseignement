{% extends "ProjetCoursBundle::layout.html.twig" %}

{% block title %}
	Accueil - {{ parent() }}
{% endblock %}

{% block Enseignement_body %}

<h1>Document creation</h1>

<form action="{{ path('enseignement_create_document', { 'id': id }) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('document') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}